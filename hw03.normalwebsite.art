server {
        listen 80;
        listen [::]:80;

        root /home/andrew/www/hw03.normalwebsite.art;

        index index.html;

        server_name hw03.normalwebsite.art;

        location / {
                # First attempt to serve request as file, then
                # as directory, then fall back to displaying a 404.
                try_files $uri $uri/ =404;
        }
}